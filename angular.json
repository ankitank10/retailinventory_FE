{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "IN": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "src/tenants/IN",
      "sourceRoot": "src/tenants/IN",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/IN",
            "index": "src/tenants/IN/index.html",
            "main": "src/tenants/IN/main.ts",
            "polyfills": "src/tenants/IN/polyfills.ts",
            "tsConfig": "src/tenants/IN/tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/tenants/IN/favicon.ico",
              "src/tenants/IN/assets"
            ],
            "styles": [
              "src/shared/assets/scss/utility/swiper.min.css",
              "src/shared/assets/scss/style.scss",
              "src/tenants/IN/assets/scss/styles-in.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/shared/assets/scss/base"
              ]
            },
            "scripts": [
              "src/shared/utils/swiper.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/tenants/IN/environments/environment.ts",
                  "with": "src/tenants/IN/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "IN:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "IN:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "IN:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/tenants/IN/test.ts",
            "polyfills": "src/tenants/IN/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/tenants/IN/favicon.ico",
              "src/tenants/IN/assets"
            ],
            "styles": [
              "src/tenants/IN/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tenants/IN/tsconfig.app.json",
              "src/tenants/IN/tsconfig.spec.json",
              "src/tenants/IN/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "IN:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "IN:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "IN"
}